{
    "name": "@vibefun/core",
    "version": "0.1.0",
    "description": "Vibefun compiler core - lexer, parser, and type system",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist/**/*.js",
        "!dist/**/*.test.js",
        "dist/**/*.d.ts",
        "!dist/**/*.test.d.ts",
        "dist/**/*.d.ts.map",
        "!dist/**/*.test.d.ts.map",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc --build",
        "build:watch": "tsc --build --watch",
        "check": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest run --coverage",
        "clean": "rm -rf dist tsconfig.tsbuildinfo"
    },
    "keywords": [
        "vibefun",
        "compiler",
        "lexer",
        "parser",
        "functional-programming"
    ],
    "author": "",
    "license": "MIT",
    "engines": {
        "node": ">=24.10.0"
    }
}
