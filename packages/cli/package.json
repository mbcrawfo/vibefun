{
    "name": "@vibefun/cli",
    "version": "0.1.0",
    "description": "Vibefun command-line interface",
    "type": "module",
    "bin": {
        "vibefun": "./dist/index.js"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*.js",
        "!dist/**/*.test.js",
        "dist/**/*.js.map",
        "!dist/**/*.test.js.map",
        "dist/**/*.d.ts",
        "!dist/**/*.test.d.ts",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc --build",
        "build:watch": "tsc --build --watch",
        "check": "tsc --noEmit",
        "dev": "tsc --watch",
        "clean": "rm -rf dist tsconfig.tsbuildinfo"
    },
    "keywords": [
        "vibefun",
        "cli",
        "compiler",
        "functional-programming"
    ],
    "author": "",
    "license": "MIT",
    "engines": {
        "node": ">=24.10.0"
    },
    "dependencies": {
        "@vibefun/core": "*",
        "commander": "12.1.0"
    }
}
